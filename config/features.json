{
  "features": {
    "PATTERN_DOMINANCE": {
      "enabled": false,
      "description": "Empire Pattern Dominance - Pattern-driven entry gating system",
      "version": "2.3.0",
      "settings": {
        "eliteThreshold": 0.75,
        "eliteSamples": 20,
        "provenThreshold": 0.55,
        "provenSamples": 10,
        "weakThreshold": 0.35,
        "eliteSizeMultiplier": 1.5,
        "provenSizeMultiplier": 1.2,
        "weakSizeMultiplier": 0.75,
        "learningSize": "PAPER_ONLY"
      }
    },
    "PATTERN_EXIT_MODEL": {
      "enabled": true,
      "description": "Pattern-based exit intelligence - enhances MaxProfitManager",
      "version": "2.3.2",
      "shadowMode": true,
      "settings": {
        "enableReversalDetection": true,
        "enablePatternTargets": true,
        "enablePatternStops": true,
        "enableMomentumExhaustion": true,
        "enableRegimeExits": true,
        "minReversalConfidence": 0.65,
        "reversalExitPercent": 0.5
      }
    },
    "PATTERN_MEMORY_PARTITION": {
      "enabled": true,
      "description": "Mode-aware pattern memory to prevent contamination",
      "version": "2.3.2",
      "settings": {
        "live": "pattern_memory.live.json",
        "paper": "pattern_memory.paper.json",
        "backtest": "pattern_memory.backtest.json",
        "backtestPersist": false
      }
    },
    "PATTERN_BASED_SIZING": {
      "enabled": true,
      "description": "Dynamic position sizing based on pattern win rates",
      "version": "2.2.0",
      "settings": {
        "minMultiplier": 0.75,
        "maxMultiplier": 1.5,
        "requiredSamples": 5
      }
    },
    "PAPER_TRADING": {
      "enabled": true,
      "description": "Paper trading mode for testing without real funds",
      "version": "2.1.3"
    },
    "CIRCUIT_BREAKER": {
      "enabled": true,
      "description": "Circuit breaker to prevent error cascades",
      "version": "2.0.0",
      "settings": {
        "maxErrors": 5,
        "resetTimeMs": 300000
      }
    },
    "TRAI_INFERENCE": {
      "enabled": false,
      "description": "TRAI AI model inference for advanced pattern recognition",
      "version": "2.1.0"
    },
    "WEBSOCKET_DASHBOARD": {
      "enabled": true,
      "description": "Real-time WebSocket dashboard updates",
      "version": "2.1.4"
    },
    "BACKTEST_API": {
      "enabled": true,
      "description": "REST API for backtesting",
      "version": "2.1.4",
      "port": 3011
    },
    "MA_EXTENSION_FILTER": {
      "enabled": false,
      "description": "20MA Extension + Acceleration filter - skip first touch after accelerating away",
      "version": "1.0.0",
      "shadowMode": true,
      "settings": {
        "slopeWindow": 5,
        "slopeTh": 0.001,
        "slope200Th": 0.0005,
        "extTh": 1.5,
        "accelTh": 0.2,
        "touchBand": 0.3,
        "skipTimeout": 20
      }
    },
    "ML_ENHANCED_SIGNALS": {
      "enabled": true,
      "description": "ML-tier enhanced signal generation with two-pole oscillator",
      "version": "1.0.0",
      "settings": {
        "volumeMultiplier": 2.0,
        "macdSensitivity": 1.0,
        "twoPoleThreshold": 0
      }
    },
    "ADVANCED_INDICATORS": {
      "enabled": true,
      "description": "Advanced technical indicators (EMA alignment, etc.)",
      "version": "1.0.0",
      "settings": {
        "emaFastPeriod": 9,
        "emaSlowPeriod": 20
      }
    },
    "ML_VOLUME_ANALYSIS": {
      "enabled": true,
      "description": "ML-tier volume analysis and confirmation",
      "version": "1.0.0",
      "settings": {
        "volumeMALength": 20,
        "volumeMultiplier": 2.0
      }
    },
    "OGZ_TPO": {
      "enabled": true,
      "description": "OGZ TPO (Time-Price-Opportunity) integration",
      "version": "1.0.0",
      "settings": {
        "mode": "aggressive",
        "dynamicSL": true,
        "confluence": true,
        "voteWeight": 0.35,
        "adaptive": true
      }
    },
    "AGGRESSIVE_LEARNING_MODE": {
      "enabled": true,
      "description": "Boost trading activity while pattern bank builds. More trades = faster learning.",
      "version": "1.0.0",
      "settings": {
        "positionSizeMultiplier": 2.0,
        "minConfidenceThreshold": 55,
        "profitTargetPercent": 1.5,
        "minHoldMinutes": 2,
        "tpoBoost": true,
        "twoPoleBoost": true,
        "volumeConfirmation": true
      }
    },
    "EXIT_SYSTEM": {
      "enabled": true,
      "description": "Single exit system selector - prevents multiple competing exits",
      "version": "1.0.0",
      "settings": {
        "activeSystem": "maxprofit",
        "availableSystems": {
          "maxprofit": "MaxProfitManager - tiered exits with trailing stops (RECOMMENDED)",
          "intelligence": "TradeIntelligenceEngine - 13-dimension analysis",
          "pattern": "PatternExitModel - pattern-based exits",
          "brain": "TradingBrain signals - brain sell + conditions",
          "legacy": "All systems active (old behavior - NOT RECOMMENDED)"
        },
        "hardStopAlwaysOn": true,
        "hardStopPercent": -1.5
      }
    }
  },
  "environment": {
    "mode": "paper",
    "debug": false,
    "logLevel": "info"
  }
}